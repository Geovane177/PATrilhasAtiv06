using System;

namespace Atividades
{
	class Program
	{
		public static void Main(string[] args)
		{
			int[] n1 = new int[5];
			Console.WriteLine("digite 5 n√∫meros");
			
			for(int i=0; i<5; i++)
			{
				n1[i] = int.Parse(Console.ReadLine());
			}
			Array.Sort(n1);
			Array.Reverse(n1);
			
			for (int j = 0; j<5; j++)
			{
				Console.WriteLine(n1[j]);
			}
			Console.Write("Pressione para continuar . . . ");
			Console.ReadKey(true);
		}
	}
}
