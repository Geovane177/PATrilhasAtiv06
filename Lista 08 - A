using System;

namespace Atividades
{
	class Program
	{
		public static void Main(string[] args)
		{
						
			for (int i = 0; i <= 100; i++) {
				if (i % 2 == 0) {
					Console.WriteLine(i);
				}
				
			}
			
			
			Console.Write("Pressione para continuar . . . ");
			Console.ReadKey(true);
		}
	}
}
